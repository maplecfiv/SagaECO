<?xml version="1.0"?>
<SagaValidation>
    <loglevel>0</loglevel>
    <Port>12006</Port>

    <!-- address of Mysql server or Path to Save for dat implementation-->
    <dbhost>172.16.160.1</dbhost>

    <!-- database name for SQL -->
    <dbname>sagaeco</dbname>

    <!-- port for databaser server(SQL or db4o) -->
    <dbport>3306</dbport>

    <!-- 
              Info = 1,
              Warning = 2,
              Error = 4,
              SQL = 8,
              Debug = 16       
    -->
    <LogLevel>31</LogLevel>

    <dbuser>root</dbuser>
    <dbpass>123456</dbpass>

    <!-- Password used for verify the connection from mapserver -->

    <Password>saga</Password>
    <!-- Packet protocoll Version -->
    <Version>Saga17</Version>

    <!-- 设置服务器IP -->
    <ServerName>SagaECO</ServerName>
    <ServerIP>172.16.160.1:12000</ServerIP>

    <!-- 服务器維護狀態 -->
    <ServerClose>False</ServerClose>

    <!-- 客戶端指紋 -->
    <clientgameversion></clientgameversion>

    <!--
    MYSQL = 0
    ACCESS = 1
    -->
    <DBType>0</DBType>

    <dbEncoding>utf8</dbEncoding>

</SagaValidation>
